<%= simple_form_for [venue, booking] do |f| %>
<div id="availabilities" data-availabilities=<%=@bookings.to_json%>></div>
  <%= f.input :event_name %>
  <div class="dates-flexed">
    <%= f.input :start_date, as: :string, required: false, input_html: {class: "datepicker"} %>
    <%= f.input :end_date, as: :string, required: false, input_html: {class: "datepicker"}  %>
  </div>
  <div class="text-center">
  <%= f.submit class:"button_blue_outline"%>
  </div>
<% end %>
